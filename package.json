{
  "name": "ai-mobile-app",
  "version": "1.0.0",
  "description": "AI Production Monitor - Mobile App built with Expo, React Native, TypeScript",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:proxy": "cross-env EXPO_PUBLIC_API_BASE=http://localhost:3001/api/data EXPO_PUBLIC_AUTH_BASE=http://localhost:3001/api/auth expo start",
    "dev": "cross-env EXPO_PUBLIC_API_BASE=http://localhost:3001/api/data EXPO_PUBLIC_AUTH_BASE=http://localhost:3001/api/auth concurrently \"npm run proxy\" \"expo start\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "proxy": "node proxy/server.js",
    "dev:reset": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts\\reset-dev.ps1"
  },
  "keywords": [
    "expo",
    "react-native",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.21",
    "@react-navigation/native-stack": "^7.6.4",
    "axios": "^1.13.2",
    "expo": "~54.0.29",
    "expo-document-picker": "^14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-secure-store": "~15.0.8",
    "expo-updates": "~29.0.15",
    "express-rate-limit": "^8.2.1",
    "morgan": "^1.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/jest": "~29.5.14",
    "@types/node": "^24.10.1",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "babel-preset-expo": "^54.0.7",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "express": "^4.19.2",
    "http-proxy-middleware": "^2.0.6",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.16",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
