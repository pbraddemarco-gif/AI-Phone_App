{
  "expo": {
    "name": "AI Production Monitor",
    "slug": "ai-production-monitor",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "aimobileapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c8a91170-3131-42db-ac96-ea600a7a48fd"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.automationintellect.aiproductionmonitor",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs access to your camera to capture photos for production action reports and documentation.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images for production action reports and documentation.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save photos you capture for production records.",
        "NSLocationWhenInUseUsageDescription": "Location access is used to tag production data with facility information.",
        "CFBundleDevelopmentRegion": "en",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false
        }
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "requireFullScreen": false,
      "userInterfaceStyle": "light"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.automationintellect.aiproductionmonitor",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photos to select images for production reports.",
          "cameraPermission": "This app needs camera access to capture photos for production reports."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c8a91170-3131-42db-ac96-ea600a7a48fd"
      }
    },
    "owner": "pbraddemarco-gif"
  }
}
