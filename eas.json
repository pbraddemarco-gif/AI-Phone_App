{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE": "http://localhost:3001/api/data",
        "EXPO_PUBLIC_AUTH_BASE": "http://localhost:3001/api/auth"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://lowcost-env.upd2vnf6k6.us-west-2.elasticbeanstalk.com",
        "EXPO_PUBLIC_AUTH_BASE": "https://app.automationintellect.com/api"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "bundler": "metro"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE": "https://lowcost-env.upd2vnf6k6.us-west-2.elasticbeanstalk.com",
        "EXPO_PUBLIC_AUTH_BASE": "https://app.automationintellect.com/api"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "brad.demarco@automationintellect.com",
        "ascAppId": "6756504159",
        "appleTeamId": "K7RD3CHTQ7"
      },
      "android": {
        "serviceAccountKeyPath": "./google-play-api-key.json",
        "track": "internal"
      }
    }
  }
}
